{
  "[html]": {
    "editor.defaultFormatter": "vscode.html-language-features"
  },
  "[json]": {
    "editor.stickyScroll.enabled": true,
    "editor.stickyScroll.maxLineCount": 10,
    "editor.wordWrap": "on"
  },
  "[markdown]": {
    "editor.fontFamily": "Monaspace Argon, Monaspace Radon, Menlo",
    "editor.quickSuggestions": {
      "other": "on"
    },
    "editor.stickyScroll.enabled": true
  },
  "[python]": {
    "editor.defaultFormatter": "charliermarsh.ruff"
  },
  "[yaml]": {
    "editor.wordWrap": "on"
  },
  "breadcrumbs.enabled": false,
  "cSpell.allowCompoundWords": true,
  "cSpell.enableFiletypes": ["toml"],
  "cSpell.ignorePaths": ["*.json", "*.csv", "*.ipynb"],
  "cSpell.minWordLength": 5,
  "cSpell.userWords": [],
  "debug.terminal.clearBeforeReusing": true,
  "diffEditor.hideUnchangedRegions.enabled": true,
  "diffEditor.ignoreTrimWhitespace": false,
  "editor.accessibilitySupport": "off",
  "editor.codeActionsOnSave": {
    "source.fixAll": "explicit",
    "source.organizeImports": "explicit"
  },
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.fontFamily": "Menlo, Monaspace Argon, Consolas, Operator Mono Lig, JetBrains Mono, Monaspace Neon, Monaspace Radon",
  "editor.fontLigatures": "'calt', 'liga', 'dlig', 'ss01', 'ss02', 'ss03', 'ss04', 'ss05', 'ss06', 'ss07', 'ss08'",
  "editor.fontSize": 14,
  "editor.formatOnSave": true,
  "editor.guides.bracketPairs": true,
  "editor.letterSpacing": 0.7,
  "editor.lineHeight": 2,
  "editor.minimap.autohide": true,
  "editor.minimap.enabled": false,
  "editor.minimap.renderCharacters": false,
  "editor.minimap.showSlider": "always",
  "editor.mouseWheelZoom": true,
  "editor.multiCursorModifier": "ctrlCmd",
  "editor.renderWhitespace": "all",
  "editor.rulers": [88, 100],
  "editor.semanticHighlighting.enabled": true,
  "editor.snippetSuggestions": "inline",
  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "scope": [
          "comment",
          "entity.name.tag.html",
          "entity.name.tag",
          "entity.other.attribute-name",
          "keyword",
          "storage.modifier",
          "storage.type.class.js",
          "storage.type",
          "support.function.builtin",
          "variable.parameter"
        ],
        "settings": {
          "fontStyle": "italic"
        }
      },
      {
        "scope": [
          "constant",
          "entity.name.type.class",
          "keyword.codetag.notation",
          "variable.other.property"
        ],
        "settings": {
          "fontStyle": "bold"
        }
      },
      {
        "scope": [
          "constant.numeric.css",
          "constant.numeric.decimal.js",
          "invalid",
          "keyword.mysql",
          "keyword.operator",
          "keyword.other.unit.px.css",
          "keyword.other"
        ],
        "settings": {
          "fontStyle": ""
        }
      },
      {
        "scope": [
          "variable.parameter.function.language.special.cls.python",
          "variable.parameter.function.language.special.self.python"
        ],
        "settings": {
          "fontStyle": "",
          "foreground": "#569CD6"
        }
      }
    ]
  },
  "emojisense.languages": {
    "*": true
  },
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "extensions.autoUpdate": false,
  "extensions.ignoreRecommendations": true,
  "files.associations": {
    "*.env": "properties",
    "*.toml": "toml"
  },
  "files.exclude": {
    "**/.ipynb_checkpoints": true,
    "**/__pycache__": true
  },
  "files.refactoring.autoSave": true,
  "jupyter.askForKernelRestart": false,
  "jupyter.interactiveWindow.textEditor.executeSelection": true,
  "jupyter.notebookFileRoot": "${workspaceFolder}",
  "jupyter.themeMatplotlibPlots": true,
  "markdown.updateLinksOnFileMove.enabled": "always",
  "markdown.validate.enabled": true,
  "notebook.cellToolbarLocation": {
    "default": "left"
  },
  "notebook.cellToolbarVisibility": "hover",
  "notebook.codeActionsOnSave": {
    "source.fixAll": "explicit",
    "source.organizeImports": "explicit"
  },
  "notebook.diff.ignoreMetadata": true,
  "notebook.diff.ignoreOutputs": true,
  "notebook.formatOnCellExecution": true,
  "notebook.lineNumbers": "on",
  "notebook.markup.fontSize": 16,
  "notebook.output.fontFamily": "Menlo",
  "notebook.output.fontSize": 15,
  "notebook.output.lineHeight": 1.1,
  "notebook.output.textLineLimit": 50,
  "notebook.output.wordWrap": true,
  "notebook.showCellStatusBar": "visibleAfterExecute",
  "notebook.stickyScroll.enabled": true,
  "projectManager.git.baseFolders": [
    "/Users/iarv/Documents/iCodes",
    "/Users/iarv/Documents/OBSIDIAN",
    "/Users/iarv/This PC"
  ],
  "python.analysis.autoFormatStrings": true,
  "python.analysis.autoImportCompletions": true,
  "python.analysis.autoSearchPaths": true,
  "python.analysis.completeFunctionParens": true,
  "python.analysis.typeCheckingMode": "basic",
  "python.languageServer": "Pylance",
  "ruff.lint.args": ["--config=~/Developer/pyproject.toml"],
  "ruff.lint.run": "onSave",
  "search.useIgnoreFiles": true,
  "security.workspace.trust.untrustedFiles": "open",
  "telemetry.telemetryLevel": "off",
  "terminal.integrated.fontFamily": "Monaspace Radon, Operator Mono Lig, JetBrains Mono, Menlo",
  "terminal.integrated.fontSize": 12,
  "testing.automaticallyOpenPeekView": "never",
  "update.mode": "manual",
  "window.commandCenter": false,
  "window.zoomLevel": 1.5,
  "workbench.activityBar.location": "hidden",
  "workbench.colorCustomizations": {
    "editorRuler.foreground": "#6663",
    "statusBar.background": "#1e1e1e",
    "tab.activeBackground": "#113",
    "terminal.selectionBackground": "#a04400"
  },
  "workbench.editor.labelFormat": "medium",
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter-notebook"
  },
  "workbench.iconTheme": "material-icon-theme",
  "workbench.layoutControl.enabled": false,
  "workbench.productIconTheme": "material-product-icons",
  "workbench.sideBar.location": "right",
  "workbench.startupEditor": "none",
  "zenMode.hideLineNumbers": false
}
