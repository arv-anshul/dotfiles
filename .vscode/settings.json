{
  "[json]": {
    "editor.stickyScroll.enabled": true,
    "editor.stickyScroll.maxLineCount": 10,
    "editor.wordWrap": "on"
  },
  "[markdown]": {
    "editor.quickSuggestions": {
      "other": "on"
    },
    "editor.stickyScroll.enabled": true
  },
  "[python]": {
    "editor.defaultFormatter": "charliermarsh.ruff"
  },
  "[yaml]": {
    "editor.wordWrap": "on"
  },
  "breadcrumbs.enabled": false,
  "cSpell.allowCompoundWords": true,
  "cSpell.enableFiletypes": ["toml"],
  "cSpell.ignorePaths": ["*.json", "*.csv", "*.ipynb"],
  "cSpell.minWordLength": 5,
  "cSpell.userWords": [],
  "debug.terminal.clearBeforeReusing": true,
  "diffEditor.hideUnchangedRegions.enabled": true,
  "diffEditor.ignoreTrimWhitespace": false,
  "editor.accessibilitySupport": "off",
  "editor.codeActionsOnSave": {
    "source.fixAll": "explicit",
    "source.organizeImports": "explicit"
  },
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.fontLigatures": "'calt', 'liga', 'dlig', 'ss01', 'ss02', 'ss03', 'ss04', 'ss05', 'ss06', 'ss07', 'ss08'",
  "editor.formatOnSave": true,
  "editor.guides.bracketPairs": true,
  "editor.inlineSuggest.suppressSuggestions": true,
  "editor.letterSpacing": 0.3,
  "editor.lineHeight": 1.8,
  "editor.minimap.autohide": true,
  "editor.minimap.enabled": false,
  "editor.minimap.renderCharacters": false,
  "editor.minimap.showSlider": "always",
  "editor.mouseWheelZoom": true,
  "editor.multiCursorModifier": "ctrlCmd",
  "editor.renderWhitespace": "all",
  "editor.rulers": [88, 100],
  "editor.semanticHighlighting.enabled": true,
  "editor.snippetSuggestions": "inline",
  "editor.tokenColorCustomizations": {
    "[Default Dark Modern]": {
      "textMateRules": [
        {
          "scope": [
            "keyword.operator.unpacking.arguments.python",
            "keyword.operator.unpacking.parameter.python",
            "variable.parameter.function.language.special.cls.python",
            "variable.parameter.function.language.special.self.python"
          ],
          "settings": {
            "foreground": "#569CD6"
          }
        },
        {
          "scope": [
            "markup.list.numbered.markdown",
            "markup.list.unnumbered.markdown",
            "punctuation.definition.list.begin.markdown"
          ],
          "settings": {
            "foreground": "#9CDCFE"
          }
        },
        {
          "scope": [
            "heading.1.markdown",
            "markup.bold.markdown",
            "constant.language.boolean.yaml"
          ],
          "settings": {
            "foreground": "#C586C0"
          }
        },
        {
          "scope": [
            "heading.3.markdown",
            "punctuation.separator.annotation.result.python"
          ],
          "settings": {
            "foreground": "#DCDCAA"
          }
        },
        {
          "scope": ["markup.quote.markdown"],
          "settings": {
            "foreground": "#6A9955"
          }
        }
      ]
    },
    "textMateRules": [
      {
        "scope": [
          "comment",
          "entity.name.tag.html",
          "entity.name.tag",
          "entity.other.attribute-name",
          "keyword",
          "storage.modifier",
          "storage.type.class.js",
          "storage.type",
          "support.function.builtin",
          "variable.parameter"
        ],
        "settings": {
          "fontStyle": "italic"
        }
      },
      {
        "scope": [
          "constant.other.ellipsis.python",
          "constant",
          "entity.name.type.class",
          "keyword.codetag.notation",
          "keyword.operator.unpacking.arguments.python",
          "keyword.operator.unpacking.parameter.python",
          "punctuation.definition.inheritance.begin.python",
          "punctuation.definition.inheritance.end.python",
          "punctuation.definition.list.begin.markdown",
          "punctuation.definition.quote.begin.markdown",
          "punctuation.separator.annotation.python",
          "punctuation.separator.annotation.result.python",
          "variable.other.property"
        ],
        "settings": {
          "fontStyle": "bold"
        }
      },
      {
        "scope": [
          "entity.name.tag.yaml",
          "invalid",
          "keyword.mysql",
          "keyword.operator",
          "variable.parameter.function.language.special.cls.python",
          "variable.parameter.function.language.special.self.python"
        ],
        "settings": {
          "fontStyle": ""
        }
      }
    ]
  },
  "emojisense.languages": {
    "*": true
  },
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "extensions.autoUpdate": false,
  "extensions.ignoreRecommendations": true,
  "files.associations": {
    "*.env": "properties",
    "*.toml": "toml"
  },
  "files.exclude": {
    "**/.*_cache": true,
    "**/.ipynb_checkpoints": true,
    "**/__pycache__": true
  },
  "files.refactoring.autoSave": true,
  "git.showPushSuccessNotification": true,
  "json.copyJsonPath.quote": "Double",
  "json.copyJsonPath.useBracketNotation": true,
  "jupyter.askForKernelRestart": false,
  "jupyter.interactiveWindow.textEditor.executeSelection": true,
  "jupyter.notebookFileRoot": "${workspaceFolder}",
  "jupyter.runStartupCommands": ["%load_ext autoreload", "%autoreload 2"],
  "jupyter.themeMatplotlibPlots": true,
  "markdown.updateLinksOnFileMove.enabled": "always",
  "markdown.validate.enabled": true,
  "notebook.cellToolbarLocation": {
    "default": "left"
  },
  "notebook.cellToolbarVisibility": "hover",
  "notebook.codeActionsOnSave": {
    "source.fixAll": "explicit",
    "source.organizeImports": "explicit"
  },
  "notebook.diff.ignoreMetadata": true,
  "notebook.diff.ignoreOutputs": true,
  "notebook.formatOnCellExecution": true,
  "notebook.lineNumbers": "on",
  "notebook.markup.fontSize": 16,
  "notebook.output.fontFamily": "Menlo",
  "notebook.output.fontSize": 15,
  "notebook.output.lineHeight": 1.1,
  "notebook.output.textLineLimit": 50,
  "notebook.output.wordWrap": true,
  "notebook.showCellStatusBar": "visibleAfterExecute",
  "notebook.stickyScroll.enabled": true,
  "projectManager.git.baseFolders": [
    "/Users/iarv/Developer",
    "/Users/iarv/Documents/OBSIDIAN"
  ],
  "python.analysis.autoFormatStrings": true,
  "python.analysis.autoImportCompletions": true,
  "python.analysis.autoSearchPaths": true,
  "python.analysis.completeFunctionParens": true,
  "python.analysis.typeCheckingMode": "basic",
  "python.languageServer": "Pylance",
  "ruff.lint.args": ["--extend-unfixable=F401"],
  "ruff.lint.run": "onSave",
  "search.useIgnoreFiles": true,
  "security.workspace.trust.untrustedFiles": "open",
  "telemetry.telemetryLevel": "off",
  "terminal.integrated.fontSize": 12,
  "testing.automaticallyOpenPeekView": "never",
  "update.mode": "manual",
  "window.commandCenter": false,
  "window.density.editorTabHeight": "compact",
  "window.zoomLevel": 1.5,
  "workbench.activityBar.location": "hidden",
  "workbench.colorCustomizations": {
    "[Default Dark Modern]": {
      "editor.background": "#1c1c1c",
      "panel.background": "#1c1c1c",
      "sideBar.background": "#1c1c1c",
      "tab.activeBackground": "#33a5",
      "tab.inactiveForeground": "#fff6"
    },
    "editorGroupHeader.tabsBackground": "#0000",
    "editorRuler.foreground": "#6664",
    "scrollbar.shadow": "#0000",
    "sideBarSectionHeader.background": "#0000",
    "sideBarSectionHeader.border": "#0000",
    "statusBar.background": "#0000",
    "tab.inactiveBackground": "#0000",
    "tree.inactiveIndentGuidesStroke": "#6668"
  },
  "workbench.editor.labelFormat": "medium",
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter-notebook"
  },
  "workbench.iconTheme": "material-icon-theme",
  "workbench.layoutControl.enabled": false,
  "workbench.productIconTheme": "material-product-icons",
  "workbench.settings.editor": "json",
  "workbench.sideBar.location": "right",
  "workbench.startupEditor": "none",
  "zenMode.hideLineNumbers": false
}
