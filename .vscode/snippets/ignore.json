{
  ".gitignore": {
    "scope": "ignore",
    "prefix": "py",
    "body": [
      "# Custom arv-anshul ignore snippet",
      "__pycache__/",
      "__rough__/",
      ".DS_Store",
      ".env",
      ".ipynb_checkpoints/",
      ".venv/",
      ".vscode/",
      "*__rough__*.*",
      "*.arv.*",
      "*.arv",
      "*.log",
      "*.pyc",
      "arv.*",
      ""
    ],
    "description": "A python repo gitignore snippet."
  },
  "RoughIgnore": {
    "scope": "ignore",
    "prefix": "rough",
    "body": ["*__rough__*.*", "*.arv", "arv.*", "*.arv.*"]
  },
  "Extra Ignore": {
    "scope": "ignore",
    "prefix": "extra",
    "body": ["data/", "logs/"],
    "description": "Extra ignore for a common python project."
  },
  ".dockerignore": {
    "scope": "ignore",
    "prefix": "docker",
    "body": ["docker-compose.yml", "Dockerfile", ".docker"]
  }
}
