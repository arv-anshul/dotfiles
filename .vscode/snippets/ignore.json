{
  "PyIgnore": {
    "scope": "ignore",
    "prefix": "py",
    "body": [
      "# Custom arv-anshul .gitignore snippet",
      "__pycache__/",
      "__rough__/",
      ".DS_Store",
      ".env",
      ".ipynb_checkpoints/",
      ".venv/",
      ".vscode/",
      "*__rough__*.*",
      "*.arv",
      "*.arv.*",
      "arv.*",
      "logs/",
      "",
      "# --- --- --- --- --- --- --- --- --- --- --- --- --- --- --- --- --- --- --- #",
      "$0"
    ],
    "description": "A python repo gitignore snippet."
  },
  "RoughIgnore": {
    "scope": "ignore",
    "prefix": "rough",
    "body": ["*__rough__*.*", "*.arv", "arv.*", "*.arv.*"]
  }
}
