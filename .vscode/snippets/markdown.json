{
  "raise issues": {
    "scope": "markdown",
    "prefix": ["issues"],
    "body": [
      "### Issues",
      "",
      "If you have any issue or query related to this project you can raise [here](https://github.com/arv-anshul/$1/issues).$0"
    ],
    "description": "Snippet for raise issues section."
  },
  "github link": {
    "scope": "markdown",
    "prefix": ["ghlink", "gha"],
    "body": "https://github.com/arv-anshul"
  },
  "html tag": {
    "scope": "markdown",
    "prefix": ["tag", "html", "ptag"],
    "body": ["<${1:p}${2}>$0</${1:p}>"]
  },
  "img tag": {
    "scope": "markdown",
    "prefix": ["img"],
    "body": ["<img src=\"${1:https://}\" ${2|width,height|}=\"${3:40}px\">"]
  },
  "dropdown section": {
    "scope": "markdown",
    "prefix": ["details", "dropdown"],
    "body": [
      "<details>",
      "<summary>${1:summary}</summary>",
      "",
      "$0",
      "",
      "</details>"
    ]
  },
  "md file properties": {
    "scope": "markdown",
    "prefix": ["ppp"],
    "body": [
      "---",
      "tags: ",
      "  - ${1:doc-tags}",
      "created-on: $CURRENT_YEAR-$CURRENT_MONTH-$CURRENT_DATE $CURRENT_HOUR:$CURRENT_MINUTE",
      "description: ${2:\"Describe the file contents.\"}",
      "---",
      ""
    ]
  }
}
