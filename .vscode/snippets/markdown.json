{
  "dropdown section": {
    "body": [
      "<details>",
      "<summary>${1:summary}</summary>",
      "",
      "$TM_SELECTED_TEXT",
      "",
      "</details>"
    ],
    "prefix": "details"
  },
  "github link": {
    "body": "https://github.com/arv-anshul",
    "prefix": "gha"
  },
  "p tag": {
    "body": "<${1:p}${2}>${3:$TM_SELECTED_TEXT}</${1:p}>$0",
    "prefix": "ptag"
  },
  "img tag": {
    "body": "<img src=\"${1:https://}\" ${2|width,height|}=\"${3:40}px\">",
    "prefix": "img"
  }
}
